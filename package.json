{
  "name": "ds-overbot",
  "version": "1.0.0",
  "description": "",
  "main": "build/index",
  "types": "build/index",
  "scripts": {
    "fix": "yarn fix:lint && yarn fix:other",
    "test": "yarn test:ts && yarn test:lint && yarn test:other",
    "fix:lint": "yarn test:lint --fix",
    "fix:other": "yarn prettier --write",
    "test:ts": "yarn build --noEmit",
    "test:lint": "eslint \"src/**/*\" --quiet",
    "test:other": "yarn prettier --list-different",
    "prettier": "prettier \"{src,test}/**/*\"",
    "dev-compose": "docker-compose -f docker-compose.dev.yml up -d --build",
    "dev": "ts-node-dev -r dotenv/config -r tsconfig-paths/register --respawn src/index.ts",
    "debug": "tsc && ts-node-dev -r dotenv/config -r tsconfig-paths/register --respawn src/index.ts",
    "start": "node -r ./tsconfig-paths-bootstrap.js dist/index.js",
    "build": "tsc"
  },
  "author": "Lyncore",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.11.0",
    "@discordx/importer": "^1.1.9",
    "@discordx/music": "^4.0.0",
    "@discordx/pagination": "^2.2.2",
    "@tsed/ajv": "^5.65.6",
    "@tsed/common": "^5.65.6",
    "@tsed/core": "^5.65.6",
    "@tsed/di": "^5.65.6",
    "@tsed/platform-express": "^5.65.6",
    "@types/moment": "^2.13.0",
    "ajv": "^6.12.6",
    "body-parser": "^1.19.0",
    "bull": "^3.18.0",
    "chalk": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "discord.js": "^13.6.0",
    "discordx": "^9.3.1",
    "express": "^4.18.1",
    "hsxkpasswd": "^1.0.1",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "6.21.3",
    "sequelize-typescript": "^2.1.3",
    "ts-httpexceptions": "^4.1.0",
    "yarn": "^1.22.18"
